@model Travel.Web.Models.Destination

@Html.AdminBreadcrumbs("Delete Destination", "Destinations", "AdminDestinationsHome")

<header class="row admin-header">
    <h2>Delete Destination</h2>
</header>

<div class="row">    

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">       
            <button type="submit" value="Delete" class="button alert" >Delete <strong>@Model.Name</strong></button>       

        </div>
        <br/>
    
    }

    @Html.BackLink("Destinations", "AdminDestinationsHome")
</div>